    {
      "id": "ne-pt-country",
      "layout": {
        "text-field": [
          "get",
          "name"
        ],
        "text-font": [
          "Roboto Regular"
        ],
        "text-size": 13
      },
      "paint": {
        "text-color": [
          "rgb",
          0,
          0,
          0
