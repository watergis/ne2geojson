  "site_root": "https://watergis.github.io/ne2geojson",
  "sources": {
    "ne": {
      "attribution": "OpenStreetMap contributors, <img src='https://unopengis.github.io/watermark/watermark.png'>",
      "maxzoom": 5,
      "tiles": [
        "https://watergis.github.io/ne2geojson/zxy/{z}/{x}/{y}.pbf"
      ],
      "type": "vector"
    }
  },
  "sprite": "https://watergis.github.io/ne2geojson/sprite/sprite",
  "version": 8,
  "zoom": 2
}
