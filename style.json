        "text-halo-color": [
          "rgb",
          238,
          238,
          238
        ],
        "text-halo-width": 1
      },
      "source": "ne",
      "source-layer": "country_annotation",
      "type": "symbol"
    },
    {
      "filter": [
        "match",
        [
          "geometry-type"
        ],
        [
          "Point",
          "MultiPoint"
        ],
        true,
        false
      ],
      "id": "ne-pt-place",
      "layout": {
        "icon-image": [
          "match",
          [
            "get",
            "place"
          ],
          "airports",
          "airport",
          "ports",
          "ferry_terminal",
          "mountains",
          "refugee",
          "city",
          "national_capital",
          "default"
        ],
        "icon-size": 1,
        "text-field": [
          "get",
          "name"
        ],
        "text-font": [
          "Roboto Regular"
        ],
        "text-justify": "auto",
        "text-radial-offset": 1,
        "text-size": 10,
        "text-variable-anchor": [
          "top",
          "bottom",
          "left",
          "right"
        ]
      },
      "paint": {
        "text-color": [
          "rgb",
          0,
          0,
          0
        ],
        "text-halo-color": [
          "rgb",
          238,
          238,
          238
        ],
        "text-halo-width": 1
      },
      "source": "ne",
      "source-layer": "place",
      "type": "symbol"
    }
