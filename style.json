      "paint": {
        "line-color": [
          "rgb",
          187,
          222,
          251
        ]
      },
      "source": "ne",
      "source-layer": "water",
      "type": "line"
    },
    {
      "filter": [
        "match",
        [
          "geometry-type"
        ],
        [
          "Polygon",
          "MultiPolygon"
        ],
        true,
        false
      ],
